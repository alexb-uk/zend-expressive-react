{
  "name": "zend-expressive-react",
  "version": "1.0.0",
  "description": "Spike skeleton for Zend Expressive and React frontend",
  "main": "public/index.php",
  "repository": "https://github.com/alexb-uk/zend-expressive-react.git",
  "author": "Alex Barnes <alexbarnes@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.6.1",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.4.1"
  },
  "scripts": {
    "dev": "webpack -d --watch",
    "build": "webpack -p"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/js/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/js/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "verbose": true
  }
}
